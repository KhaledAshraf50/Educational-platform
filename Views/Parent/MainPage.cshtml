        <div class="feild" data-field="name">
            <i class="fa-solid fa-id-card"></i>
            <div class="part2-feild">
                <p>الاسم</p>
                <h1>@Model.User.fname @Model.User.lastName</h1>
            </div>
        </div>
        <div class="feild" data-field="email">
            <i class="fa-solid fa-envelope"></i>
            <div class="part2-feild">
                <p>البريد الإلكتروني</p>
                <h1>@Model.User.Email</h1>
            </div>
        </div>

        <div class="feild" data-field="phone">
            <i class="fa-solid fa-phone"></i>
            <div class="part2-feild">
                <p>رقم الهاتف</p>
                <h1>@Model.User.PhoneNumber</h1>
            </div>
        </div>
        <div class="feild" data-field="password">
            <i class="fa-solid fa-key"></i>
            <div class="part2-feild">
                <p>كلمة المرور</p>
